    "@web5/api": "^0.11.0",
    "@web5/browser": "^0.0.3",
    "@web5/common": "^1.0.2",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "pinia": "^2.1.7",
